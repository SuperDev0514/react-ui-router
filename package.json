{
  "name": "@uirouter/react",
  "version": "0.5.2",
  "description": "State based routing for React",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "jsnext:main": "lib-esm/index.js",
  "scripts": {
    "test": "jest --watch",
    "test:ci": "NODE_ENV=production jest  --silent",
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 8000 --config ./examples/typescript/webpack.config.js --history-api-fallback --content-base examples/typescript",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && cross-env NODE_ENV=production webpack && npm run fixmaps",
    "release": "release",
    "fixmaps": "modify_sourcemap_paths",
    "clean": "shx rm -rf _bundles lib lib-esm build",
    "docs": "./scripts/docs.js",
    "package": "npm run build",
    "prepublish": "npm run build",
    "artifacts": "artifact_tagging",
    "changelog": "update_changelog --include-core",
    "prettier": "node ./scripts/prettier.js write-changed",
    "prettier-all": "node ./scripts/prettier.js write"
  },
  "homepage": "https://ui-router.github.io/react",
  "contributors": [
    {
      "name": "Marco Botto",
      "web": "https://github.com/elboman"
    },
    {
      "name": "Chris Thielen",
      "web": "https://github.com/christopherthielen"
    }
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/react.git"
  },
  "dependencies": {
    "@uirouter/core": "=5.0.7",
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.0.0 || ^16.0.0-alpha || 16.0.0-rc.2"
  },
  "devDependencies": {
    "@types/classnames": "0.0.31",
    "@types/lodash": "^4.14.64",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@uirouter/publish-scripts": "^1.0.9",
    "awesome-typescript-loader": "^3.2.3",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chalk": "^2.0.1",
    "conventional-changelog-cli": "1.1.1",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "glob": "^7.1.2",
    "jest": "^20.0.3",
    "prettier": "^1.5.2",
    "prop-types": "^15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-test-renderer": "^15.3.1",
    "readline-sync": "^1.4.7",
    "replace-in-file": "^2.0.3",
    "shelljs": "^0.7.7",
    "shx": "^0.1.4",
    "sinon": "^2.3.1",
    "ts-jest": "^20.0.4",
    "typedoc": "^0.8.0",
    "typedoc-plugin-external-module-name": "^1.0.8",
    "typedoc-plugin-internal-external": "^1.0.8",
    "typedoc-plugin-ui-router": "^1.0.6",
    "typescript": "^2.5.2",
    "ui-router-typedoc-themes": "^1.0.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.1"
  },
  "jest": {
    "rootDir": "src",
    "transform": {
      ".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost:4000",
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    }
  }
}
