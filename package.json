{
  "name": "ui-router-react",
  "version": "0.2.3",
  "description": "State based routing for React",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "dev": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 8000 --config webpack.example.js --history-api-fallback --content-base example",
    "build": "rimraf _bundles && mkdirp _bundles && NODE_ENV=production webpack",
    "prepare-package": "node scripts/package"
  },
  "homepage": "https://ui-router.github.io",
  "contributors": [
    {
      "name": "Marco Botto",
      "web": "https://github.com/elboman"
    },
    {
      "name": "Chris Thielen",
      "web": "https://github.com/christopherthielen"
    }
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/react.git"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-shim": "^0.35.1",
    "ui-router-core": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "enzyme": "^2.4.1",
    "glob": "^7.0.5",
    "jest": "^15.1.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.0.2",
    "react-test-renderer": "^15.3.1",
    "rimraf": "^2.5.3",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.5",
    "ts-jest": "^0.1.3",
    "ts-loader": "^0.8.2",
    "tsc": "^1.20150623.0",
    "typescript": "^1.8.10",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "rootDir": "src",
    "scriptPreprocessor": "../node_modules/ts-jest/preprocessor.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "globals": {
      "__TS_CONFIG__": {}
    }
  }
}
